{
	"name": "fbm",
	"version": "1.0.0",
	"description": "Facebook Messenger desktop app",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"build": "electron-builder"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"electron-window-state": "^5.0.3"
	},
	"devDependencies": {
		"electron": "latest",
		"electron-builder": "latest"
	},
	"build": {
		"appId": "com.electron.fbmessenger",
		"productName": "Facebook Messenger",
		"publish": null,
		"directories": {
			"output": "dist"
		},
		"win": {
			"target": "nsis",
			"icon": "icon.ico",
			"artifactName": "${productName}.${ext}"
		}
	}
}
